#include "xgpio_l.h"
#include "fsl.h"


#include "dds_phase_accum.h"

static uint32_t freq_to_accum(uint32_t freq_hz) {
	return freq_hz/DDS_SAMPLE_RATE;
}

void set_dds_accum(uint32_t freq_hz) {
	uint32_t accum_reg_val = freq_to_accum(accum) & DDS_ACCUM_MASK;
	putfslx(accum_reg_val, FSL_ID, FSL_DEFAULT);
}
